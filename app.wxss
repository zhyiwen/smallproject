/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.footer~.content {
  border-bottom: 110rpx solid transparent;
}
.clearfix::after{
  content: '';
  display: block;
  clear: both;
}
page{background: #f2f2f2;}
/*footer*/
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 20;
  width: 100%;
  height: 110rpx;
  background: #fff;
  border-top: 1px solid #b2b2b2;
}
.footer-list-group{
  display: flex;
  width: 100%;
}
.footer-list-group .footer-list{
  flex: 1;
  text-align: center;
  padding: 12rpx 0;
}
.footer-list .foot-img{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  margin-top: 8rpx;
  background-size: cover;
  background-repeat: no-repeat;
}
.footer-list .foot1{background: url(data:image/svg+xml;base64,)}
.footer-list .foot2{background: url(data:image/svg+xml;base64,)}
.footer-list .foot3{background: url(data:image/svg+xml;base64,)}
.footer-list .foot4{background: url(data:image/svg+xml;base64,)}
.footer-list.active .foot1{background: url(data:image/svg+xml;base64,)}
.footer-list.active .foot2{background: url(data:image/svg+xml;base64,)}
.footer-list.active .foot3{background: url(data:image/svg+xml;base64,)}
.footer-list.active .foot4{background: url(data:image/svg+xml;base64,)}
.footer-list .foot-text{
  color: #737373;
  font-size: 12px;
  display: block;
  text-align: center;
  margin-top: -2rpx;
}
.footer-list.active .foot-text{color: #4081d6;}