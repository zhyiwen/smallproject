/**index.wxss**/
page{background: #f2f2f2;}
.banner{
  width: 100%;
  height: 115px;
  padding: 20px 0;
  text-align: center;
  background: url(https://img.alicdn.com/imgextra/i1/2590951958/TB2jkvEjS8YBeNkSnb4XXaevFXa_!!2590951958.jpg) no-repeat center;
  background-size: 100%;
}
.search-box{
  position: relative;
  height: 35px;
  margin: 0 30px;
  background: #fff;
  border-radius: 5px;
  font-size: 14px;
  text-align: left;
}
.search-box input{
  color: #4e4e4e;
  height: 35px;
  padding-left: 30px;
}
.search-box .icon-search{
  position: absolute;
  left: 0;
  top: 0;
  width: 30px;
  height: 35px;
  background: url(https://img.alicdn.com/imgextra/i1/2590951958/TB2ktwYr4GYBuNjy0FnXXX5lpXa_!!2590951958.png) no-repeat center center;
}
.search-box .icon-sao{
  position: absolute;
  right: 0;
  top: 0;
  width: 35px;
  height: 35px;
  border-left: 1px solid #eee;
  background: url(https://img.alicdn.com/imgextra/i4/2590951958/TB2HwmPjLiSBuNkSnhJXXbDcpXa_!!2590951958.png) no-repeat center center #fff;
}
.search-box .icon-shuohua{
  position: absolute;
  right: 35px;
  top: 0;
  height: 35px;
  width: 30px;
  background: url(https://img.alicdn.com/imgextra/i2/2590951958/TB2gR7hr25TBuNjSspcXXbnGFXa_!!2590951958.png) no-repeat center center;
}
.banner-text{
  width: 165px;
  height: 32px;
  margin: 30px auto;
}
.pro{
  padding: 10px 20px;
  clear: both;
  background: #fff;
}
.pro .pro-list{
  width: 25%;
  float: left;
  text-align: center;
  padding: 10px 0;
}
.pro .pro-list .pro-img{
  display: block;
  width: 32px;
  height: 32px;
  margin: auto;
}
.pro .pro-list .pro-text{
  color: #000;
  font-size: 14px;
}
.intention{
  position: relative;
  padding: 10px 20px;
  border-top: 1px solid #e4e4e4;
  font-size: 14px;
  background: #fff;
  box-sizing: border-box;
  margin-bottom: 5px;
}
.intention .intention-title{
  display: inline-block;
  width: 20%;
  font-size: 15px;
  font-weight: bold;
  color: #4483d7;
  border-right: 1px solid #e4e4e4;
}
.intention .intention-a{
  display: inline-block;
  padding: 0 10px;
  width: 70%;
}
.next-icon{
  position: absolute;
  top: 50%;
  margin-top: -8px;
  right: 15px;
  background: url(https://img.alicdn.com/imgextra/i2/2590951958/TB2PAZtr7OWBuNjSsppXXXPgpXa_!!2590951958.png) no-repeat center center;
  width: 16px;
  height: 16px;
}
.sort{
  padding: 10px;
  background: #fff;
}
.sort .sort-list{
  width: 50%;
  float: left;
  padding: 5px 10px;
  box-sizing: border-box;
}
.sort .sort-list image{
  width: 100%;
}
.house{
  background: #fff;
}
.house .house-hd{
  position: relative;
  padding: 0 20px;
}
.house .house-hd .house-title{
  height: 40px;
  line-height: 40px;
  font-size: 15px;
  color: #000;
  font-weight: bold;
}
.house .house-hd .house-desc{
  float: right;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
  margin-right: 15px;
  color: #999;
}
.house .house-bd{}
.house-bd .house-bd-list{
  border-bottom: 1px solid #efeff4;
  padding: 20px;
  box-sizing: border-box;
}
.house-bd-list .house-list-img{
  width: 30%;
  height: 80px;
  border-radius: 4px;
  overflow: hidden;
}
.house-bd-list .house-list-text{
  width: 70%;
  padding-left: 20px;
  box-sizing: border-box;
  display: inline-block;
}
.house-list-text .text-title{
  font-size: 15px;
  font-weight: bold;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: keep-all;
  white-space: nowrap;
  display: block;
}
.house-list-text .text-desc{
  display: block;
  font-size: 12px;
  color: #7c7c7c;
}
.house-list-text .text-icon-group{}
.house-list-text .text-icon-group .text-icon{
  padding: 1px 5px;
  border-radius: 4px;
  color: #fff;
  margin-right: 10px;
  font-size: 10px;
}
.house-list-text .text-icon-group .icon1{
  background: linear-gradient(to right,#fa786b,#fb4d5b);
}
.house-list-text .text-icon-group .icon2{
  background: linear-gradient(to right,#3b9eff,#4081d6);
}
.house-list-text .text-icon-group .icon3{
  background: #fff;
  color: #4081d6;
  border:1px solid #4081d6;
}
.house-list-text .text-prince{
  font-size: 16px;
  color: #f95353;
  margin-right: 5px;
}
.house-list-text .text-square{
  font-size: 12px;
  color: #7c7c7c;
}

/*footer*/
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 55px;
  background: #fff;
  border-top: 1px solid #b2b2b2;
}
.footer-list-group{
  display: flex;
  width: 100%;
}
.footer-list-group .footer-list{
  flex: 1;
  text-align: center;
  padding: 6px 0;
}
.footer-list .foot-img{
  display: inline-block;
  width: 20px;
  height: 20px;
  background-repeat: no-repeat;
}
.footer-list .foot1{background: url(https://img.alicdn.com/imgextra/i4/2590951958/TB2hiU_r79WBuNjSspeXXaz5VXa_!!2590951958.png)}
.footer-list .foot2{background: url(https://img.alicdn.com/imgextra/i2/2590951958/TB2mvAGrVGWBuNjy0FbXXb4sXXa_!!2590951958.png)}
.footer-list .foot3{background: url(https://img.alicdn.com/imgextra/i4/2590951958/TB2pndXseuSBuNjSsziXXbq8pXa_!!2590951958.png)}
.footer-list .foot4{background: url(https://img.alicdn.com/imgextra/i2/2590951958/TB2Ths7r29TBuNjy1zbXXXpepXa_!!2590951958.png)}
.footer-list.active .foot1{background: url(https://img.alicdn.com/imgextra/i3/2590951958/TB2Yyogr3mTBuNjy1XbXXaMrVXa_!!2590951958.png)}
.footer-list.active .foot2{background: url(https://img.alicdn.com/imgextra/i3/2590951958/TB2qLq.sntYBeNjy1XdXXXXyVXa_!!2590951958.png)}
.footer-list.active .foot3{background: url(https://img.alicdn.com/imgextra/i1/2590951958/TB2NP1hsmBYBeNjy0FeXXbnmFXa_!!2590951958.png)}
.footer-list.active .foot4{background: url(https://img.alicdn.com/imgextra/i3/2590951958/TB2.kLwjLiSBuNkSnhJXXbDcpXa_!!2590951958.png)}
.footer-list .foot-text{
  color: #737373;
  font-size: 12px;
  display: block;
  text-align: center;
}
.footer-list.active .foot-text{color: #4081d6;}