/**index.wxss**/
.banner{
  width: 100%;
  height: 230rpx;
  padding: 40rpx 0;
  text-align: center;
  background: url(https://img.alicdn.com/imgextra/i1/2590951958/TB2jkvEjS8YBeNkSnb4XXaevFXa_!!2590951958.jpg) no-repeat center;
  background-size: 100%;
}
.search-box{
  position: relative;
  height: 70rpx;
  margin: 0 60rpx;
  background: #fff;
  border-radius: 5px;
  font-size: 14px;
  text-align: left;
}
.search-box input{
  color: #4e4e4e;
  height: 70rpx;
  padding: 0 120rpx 0 60rpx;
}
.search-box .icon-search{
  position: absolute;
  left: 0;
  top: 0;
  width: 60rpx;
  height: 70rpx;
  background: url(data:image/svg+xml;base64,) no-repeat center center;
}
.search-box .icon-sao{
  position: absolute;
  right: 0;
  top: 0;
  width: 70rpx;
  height: 70rpx;
  border-left: 1px solid #eee;
  background: url(data:image/svg+xml;base64,) no-repeat center center #fff;
}
.search-box .icon-shuohua{
  position: absolute;
  right: 70rpx;
  top: 0;
  height: 70rpx;
  width: 60rpx;
  background: url(data:image/svg+xml;base64,) no-repeat center center;
}
.banner-text{
  width: 540rpx;
  height: 69rpx;
  margin: 60rpx auto;
}
.pro{
  padding: 20rpx 40rpx;
  clear: both;
  background: #fff;
}
.pro .pro-list{
  width: 25%;
  float: left;
  text-align: center;
  padding: 20rpx 0;
}
.pro .pro-list .pro-img{
  display: block;
  width: 64rpx;
  height: 64rpx;
  margin: auto;
}
.pro .pro-list .pro-text{
  color: #000;
  font-size: 14px;
}
.intention{
  position: relative;
  padding: 20rpx 40rpx;
  border-top: 1px solid #e4e4e4;
  font-size: 14px;
  background: #fff;
  box-sizing: border-box;
  margin-bottom: 10rpx;
}
.intention .intention-title{
  display: inline-block;
  width: 20%;
  font-size: 15px;
  font-weight: bold;
  color: #4483d7;
  border-right: 1px solid #e4e4e4;
}
.intention .intention-a{
  display: inline-block;
  padding: 0 20rpx;
  width: 70%;
}
.next-icon{
  position: absolute;
  top: 50%;
  margin-top: -16rpx;
  right: 30rpx;
  background: url(data:image/svg+xml;base64,) no-repeat center center;
  width: 32rpx;
  height: 32rpx;
}
.sort{
  padding: 20rpx;
  background: #fff;
}
.sort .sort-list{
  width: 50%;
  float: left;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.sort .sort-list image{
  width: 100%;
}
.house{
  background: #fff;
}
.house .house-hd{
  position: relative;
  padding: 0 40rpx;
}
.house .house-hd .house-title{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 15px;
  color: #000;
  font-weight: bold;
}
.house .house-hd .house-desc{
  float: right;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 12px;
  margin-right: 30rpx;
  color: #999;
}
.house-bd .house-bd-list{
  border-bottom: 1px solid #efeff4;
  padding: 40rpx;
  box-sizing: border-box;
}
.house-bd-list .house-list-img{
  width: 30%;
  height: 160rpx;
  border-radius: 4px;
  overflow: hidden;
}
.house-bd-list .house-list-text{
  width: 70%;
  padding-left: 40rpx;
  box-sizing: border-box;
  display: inline-block;
}
.house-list-text .text-title{
  font-size: 15px;
  font-weight: bold;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: keep-all;
  white-space: nowrap;
  display: block;
}
.house-list-text .text-desc{
  display: block;
  font-size: 12px;
  color: #7c7c7c;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: keep-all;
  white-space: nowrap;
}
.house-list-text .text-icon-group .text-icon{
  padding: 1px 5px;
  border-radius: 4px;
  color: #fff;
  margin-right: 20rpx;
  font-size: 10px;
}
.house-list-text .text-icon-group .icon1{
  background: linear-gradient(to right,#fa786b,#fb4d5b);
}
.house-list-text .text-icon-group .icon2{
  background: linear-gradient(to right,#3b9eff,#4081d6);
}
.house-list-text .text-icon-group .icon3{
  padding: 0px 4px;
  background: #fff;
  color: #4081d6;
  border:1px solid #4081d6;
}
.house-list-text .text-prince{
  font-size: 15px;
  color: #f95353;
  margin-right: 10rpx;
}
.house-list-text .text-square{
  font-size: 12px;
  color: #7c7c7c;
}

